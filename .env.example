# 텔레그램 봇 설정
BOT_TOKEN=your_telegram_bot_token_here

# 구글 시트 설정
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_CREDENTIALS={"type":"service_account","project_id":"your_project_id",...}

# 업비트 API 설정
# https://upbit.com/service_center/open_api_guide 에서 API 키 발급
UPBIT_ACCESS_KEY=your_upbit_access_key_here
UPBIT_SECRET_KEY=your_upbit_secret_key_here

# 코인원 API 설정
# https://coinone.co.kr/user/api/guide 에서 API 키 발급
COINONE_ACCESS_TOKEN=your_coinone_access_token_here
COINONE_SECRET_KEY=your_coinone_secret_key_here

# 바이낸스 API 설정
# https://www.binance.com/en/my/settings/api-management 에서 API 키 발급
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
